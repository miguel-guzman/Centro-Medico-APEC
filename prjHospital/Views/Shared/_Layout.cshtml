<!DOCTYPE html>
<html lang="es-do">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title | Centro Médico APEC</title>
  <!-- Stylesheets-->
  <!-- bootstrap 4.3.1 -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css">
  @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha256-YLGeXaapI0/5IgZopewRJcFXomhRMlYYjugPLSyNjTY=" crossorigin="anonymous">*@
  <!-- template -->
  <link href="~/Static/Stylesheets/styles.css" rel="stylesheet" />
  <!-- other -->
  @RenderSection("stylesheets", required: false)
  <!-- /Scripts -->
  <!-- /Stylesheets -->
</head>
<body>
  <div id="wrapper">
    <!-- Nav -->
    <nav class="navbar navbar-expand-xl navbar-light bg-light">
      @Html.ActionLink(linkText: "Centro Médico APEC", actionName: "Index", controllerName: "Home", routeValues: null, htmlAttributes: new { @class = "navbar-brand" })
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav mx-auto">
          <li class="nav-item">
            <a class="nav-link" href="@Url.Action(actionName: "Index", controllerName: "Home", routeValues: null)"><i class="mr-2 pb-1" data-feather="home"></i>Inicio</a>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              <i class="mr-2 pb-1" data-feather="triangle"></i>Actividades
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
              <a class="dropdown-item" href="@Url.Action(actionName: "Index", controllerName: "Home", routeValues: null)"><i class="mr-2 pb-1" data-feather="triangle"></i>Actividades</a>
            </div>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              <i class="mr-2 pb-1" data-feather="archive"></i>Archivo
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
              <a class="dropdown-item" href="@Url.Action(actionName: "Index", controllerName: "Personas", routeValues: null)"><i class="mr-2 pb-1" data-feather="users"></i>Pacientes</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="users"></i>Personal Sanitario</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="package"></i>Productos</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="square"></i>Habitaciones</a>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="grid"></i>Unidades</a>
            </div>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              <i class="mr-2 pb-1" data-feather="settings"></i>Configuración
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="settings"></i>Actividades</a>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="settings"></i>Especialidades</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="settings"></i>Tipos de Ingreso</a>
              <a class="dropdown-item" href="#"><i class="mr-2 pb-1" data-feather="settings"></i>Tipos de Egreso</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="@Url.Action(actionName: "Index", controllerName: "Sexos", routeValues: null)"><i class="mr-2 pb-1" data-feather="settings"></i>Sexos</a>
            </div>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="@Url.Action(actionName: "About", controllerName: "Home", routeValues: null)"><i class="mr-2 pb-1" data-feather="info"></i>Acerca de</a>
          </li>
          @*
          <li class="nav-item">
            <a class="nav-link disabled" href="@Url.Action(actionName: "Contact", controllerName: "Home", routeValues: null)" tabindex="-1"><i class="mr-2 pb-1" data-feather="help-circle"></i>Ayuda</a>
          </li>
          *@
        </ul>
        <!-- Login -->
        @Html.Partial("_LoginPartial")
        <!-- /Login -->
      </div>
    </nav>
    <!-- /Nav -->
    <!-- Body -->
    <div id="content">
      @RenderBody()
    </div>
    <!-- /Body -->
    <!-- Footer -->
    <footer class="bg-light">
      <p class="lead text-center my-0">&copy; 2019-1 - ISO710-41073 - Grupo 6 </p>
    </footer>
  </div>
  <!-- /Footer -->
  <!-- Scripts -->
  <!-- jquery 3.3.1 -->
  <script src="https://cdn.jsdelivr.net/npm/jquery@3.3.1/dist/jquery.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
  <!-- popper.js 1.14.7 -->
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha256-ZvOgfh+ptkpoa2Y4HkRY28ir89u/+VRyDE7sB7hEEcI=" crossorigin="anonymous"></script>
  <!-- bootstrap 4.3.1 -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha256-CjSoeELFOcH0/uxWu6mC/Vlrc1AARqbm/jiiImDGV3s=" crossorigin="anonymous"></script>
  <!-- feather icons 4.19.0 -->
  <script src="https://cdn.jsdelivr.net/npm/feather-icons@4.19.0/dist/feather.min.js" integrity="sha256-q1CfnFGILjNzpAyahEetCYbxcuv0WEJfK55GoD8U3lI=" crossorigin="anonymous"></script>
  <!-- template -->
  <script src="~/Static/Scripts/scripts.js"></script>
  <!-- other -->
  @RenderSection("scripts", required: false)
  <!-- /Scripts -->
</body>
</html>
